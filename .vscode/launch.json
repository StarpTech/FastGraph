{
    "configurations": [
      {
        "name": "Miniflare (npm)",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"], // same script name as in package.json
        "skipFiles": ["<node_internals>/**"],
        "sourceMaps": true,
        "env": {
            "NODE_OPTIONS":"--openssl-legacy-provider"
        }
      }
    ]
  }
  